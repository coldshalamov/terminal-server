# Example environment variables for terminal-connector
# Copy this file to .env and fill in your values

# Server URL (your Render app URL)
CONNECTOR_URL=https://your-app.onrender.com

# JWT token (generated from server's /api/session endpoint)
CONNECTOR_TOKEN=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...

# Default shell (optional, defaults to SHELL env var or /bin/bash)
CONNECTOR_SHELL=/bin/bash

# Reconnection settings (optional)
RECONNECT_INTERVAL=2000
MAX_RECONNECT_ATTEMPTS=10

# Environment variables for PTY (optional)
TERM=xterm-256color
LANG=en_US.UTF-8
